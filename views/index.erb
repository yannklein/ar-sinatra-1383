<h1>Hello from the View</h1>
<div class="container">
  <div class="row">
  <% @pokes.each do |poke| %>
  <div class="col-4">
    <div class="card">
      <img class="card-img-top" src="<%= poke.image_url %>" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title"><%= poke.name %></h5>
        <p class="card-text"><%= poke.poke_type %></p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
      </div>
    </div>
  </div>
  <% end %>
  </div>
</div>
